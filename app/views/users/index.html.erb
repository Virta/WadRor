<h1>Listing users</h1>

<table>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.username, user %></td>
        <td><%= link_to 'Destroy', user, method: :delete, data:
                { confirm: 'Are you sure?' } if current_user.is_admin? if current_user%>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path unless current_user %>
