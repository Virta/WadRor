<h1>Listing users</h1>

<table class="table table-hover">

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.username, user %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path , class:'btn btn-primary' unless current_user %>
